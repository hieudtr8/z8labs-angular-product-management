{
  "dataconnect": {
    "source": "dataconnect"
  },
  "hosting": {
    "headers": [
      {
        "source": "**/*.@(jpg|jpeg|gif|png)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
